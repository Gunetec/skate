{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Web": {
        "Url": "http://*:80",
        "Protocols": "Http1"
      }
    }
  },
  "ConnectionStrings": {
    "Skateboard3Context": "Data Source=skateboard3server.db"
  },
  "Skateboard": {
    "Blaze": {
      "PublicHost": "localhost",
      "PublicIp": "127.0.0.1",
      "QosHosts": []
    },
    "Web": {
      "BlobStorageLocation": "data\\blobs"
    },
    "Flags": {
      "EnableTicketValidation": true
    }
  }
}
